    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <SolidColorBrush x:Key="ComboBoxItemSelectedBackgroundThemeBrush">Gray</SolidColorBrush>
                    <SolidColorBrush x:Key="ComboBoxItemSelectedPointerOverBackgroundThemeBrush">Gray</SolidColorBrush>
